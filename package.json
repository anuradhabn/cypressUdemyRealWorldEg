{
  "name": "cypress_cu",
  "version": "1.0.0",
  "description": "Creighton University Test Cases",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "start": "ng serve",
    "start:win32": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve",
    "start:darwin:linux": "export NODE_OPTIONS=--openssl-legacy-provider && ng serve",
    "build": "ng build",
    "build:prod": "npm run build -- --prod --aot",
    "test": "ng test",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint ngx-admin-demo --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "delete:reports": "rm -r cypress/results/* || true",
    "prereport": "npm run delete:reports",
    "mochawesome:merge": "npx mochawesome-merge cypress/results/mochawesome/mochawesome* > mochawesome.json && npx marge mochawesome.json",
    "junit:merge": "npx junit-merge -d cypress/results/junit -o cypress/results/junit/result.xml",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "prepush": "npm run lint:ci",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "cypress:run": "npx cypress run",
    "cypress:e2e": "start-test start http-get://localhost:4200 cypress:run",
    "cypress:qa": "npx cypress open --env userName=QATester@gmail.com,password=QATester",
    "cypress:workingqa": "npx cypress open --env userName=anu.bengaluru@gmail.com,password=Ganapathi2024! ; npx cypress run ; npm run junit:merge ; npm run mockawesome:merge",
    "cypress:complete": "start https://angular.realworld.how/ npx cypress run --env userName=anu.bengaluru@gmail.com,password=Ganapathi2024! ; npm run junit:merge ; npm run mockawesome:merge",
    "cy:open_process": "npx cypress open --env userName=$DB_USERNAME,password=$PASSWORD",
    "cy:run_speccrossbrowser": "npx cypress run --spec 'cypress/e2e/Test.spec.cy.js' --browser chrome --headless --record --key fc49f653-16cf-4732-bc7e-747fece2942f",
    "cy:run_specwebkit": "npx cypress run --spec 'cypress/e2e/Test.spec.cy.js' --browser webkit --headed",
    "cy:run2browsers": "npx cypress run --spec 'cypress/e2e/Test.spec.cy.js' --browser firefox --group firefox --ci-build-id 1 & npx cypress run --spec 'cypress/e2e/Test.spec.cy.js' --browser chrome --group chrome --ci-build-id 1 --record --key fc49f653-16cf-4732-bc7e-747fece2942f"
  },
  "author": "ABN",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.6.1",
    "cypress-multi-reporters": "^1.6.4",
    "junit-merge": "^2.0.0",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "otplib": "^12.0.1",
    "playwright-webkit": "1.34",
    "start-server-and-test": "^2.0.3"
  },
  "dependencies": {
    "cypress-plugin-snapshots": "^1.4.4"
  }
}
